{
  "extends": "../../tslint.json",
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "lib",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "sq",
      "kebab-case"
    ],
    "component-class-suffix": false,
    "no-output-on-prefix": false
  }
}
